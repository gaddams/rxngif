<div class="page-header">
  <h1>Pictures</h1>
</div>

<% @list_of_all_pictures.each do |picture| %>
	<div class="row">
  		<div class="panel-heading">
          <h3 class="panel-title"><%= picture.caption %></h3>
        </div>
        <div class="panel-body">
          <img src="<%= picture.source %>" class="img-responsive" width = 200>
        </div>
    </div>

<% end %>